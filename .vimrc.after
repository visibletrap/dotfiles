set tags=~/.tags
set relativenumber

let g:buffergator_viewport_split_policy='T'
let g:buffergator_split_size=15

imap jk  <esc>
imap kj <esc>

vmap > >gv 
vmap < <gv

set wildignore+=*/tmp*
set relativenumber
set nonumber

color jellybeans+

let $LOCALVIMRC = $HOME.'/.vimrc.local'
if filereadable($LOCALVIMRC)
  source $LOCALVIMRC
endif

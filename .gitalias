[alias]
  b = branch
  br = branch -r
  ba = branch -a
  bd = branch -d
  cb = checkout -b
  ci = commit
  cm = commit -m
  cam = commit -am
  ca = commit --amend
  car = commit --amend --reset-author
  co = checkout
  cof = checkout -f
  com = checkout master
  f = fetch
  s = status
  so = show
  a = add
  aa = add -A :/
  ap = add -p
  sh = stash
  shp = stash pop
  pl = pull
  ps = push
  pfl = push --force-with-lease
  pr = pull --rebase
  prp = pull --rebase -p
  r = rebase
  ri = rebase -i
  l = log
  lp = log -p
  rom = reset --hard origin/master

[alias]
  b = branch
  cb = checkout -b
  ci = commit
  cm = commit -m
  cam = commit -am
  ca = commit --amend
  car = commit --amend --reset-author
  co = checkout
  s = status
  so = show
  a = add
  aa = add -A :/
  ap = add -p
  cb = checkout -b
  sh = stash
  shp = stash pop
  pl = pull
  ps = push
  pr = pull --rebase
  r = rebase
  ri = rebase -i
  l = log
  lp = log -p

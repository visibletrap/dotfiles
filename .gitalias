[alias]
  b = branch
  br = branch -r
  ba = branch -a
  bd = branch -d
  bm = branch -m
  cl = clone
  cb = checkout -b
  ci = commit
  cm = commit -m
  cam = commit -am
  ca = commit --amend --reset-author
  co = checkout
  cf = checkout -f
  com = checkout master
  c = checkout -
  m = merge
  mof = merge --no-ff
  f = fetch
  s = status
  so = show
  a = add
  aa = add -A :/
  ap = add -p
  sh = stash
  shp = stash pop
  pl = pull
  ps = push
  pfl = push --force-with-lease
  pr = pull --rebase
  prp = pull --rebase -p
  r = rebase
  ri = rebase -i
  rs = reset
  rh = reset --hard
  rhr = reset --hard @{u}
  l = log
  lp = log -p
